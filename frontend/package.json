{
  "name": "postfixblocker-ui",
  "version": "0.1.0",
  "scripts": {
    "start": "ng serve --host 127.0.0.1 --proxy-config proxy.json",
    "start:devc": "ng serve --host 0.0.0.0 --proxy-config proxy.json",
    "start:db2": "ng serve --host 127.0.0.1 --proxy-config proxy.json",
    "build": "ng build",
    "test": "ng test --watch=false --code-coverage",
    "test:watch": "ng test --watch",
    "lint": "eslint \"src/**/*.{ts,js}\" \"e2e/**/*.ts\" --ext .ts,.js",
    "lint:fix": "eslint \"src/**/*.{ts,js}\" \"e2e/**/*.ts\" --ext .ts,.js --fix",
    "format": "prettier --write .",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:db2": "playwright test",
    "e2e:db2:ui": "playwright test --ui",
    "mutation-test": "stryker run",
    "mutation-test:force": "stryker run -c stryker.force.conf.json"
  },
  "dependencies": {
    "@angular/animations": "^20.3.2",
    "@angular/cdk": "^20.2.5",
    "@angular/common": "^20.3.2",
    "@angular/core": "^20.3.2",
    "@angular/forms": "^20.3.2",
    "@angular/material": "^20.2.5",
    "@angular/platform-browser": "^20.3.2",
    "@angular/platform-browser-dynamic": "^20.3.2",
    "@angular/router": "^20.3.2",
    "@simplewebauthn/browser": "^13.2.0",
    "npm-check-updates": "^18.3.0",
    "rxjs": "^7.8.2",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.3",
    "@angular/cli": "^20.3.3",
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.55.1",
    "@stryker-mutator/core": "^9.1.1",
    "@stryker-mutator/karma-runner": "^9.1.1",
    "@stryker-mutator/typescript-checker": "^9.1.1",
    "@types/eslint-plugin-security": "^3.0.0",
    "@types/jasmine": "~5.1.9",
    "angular-eslint": "20.3.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^60.4.0",
    "eslint-plugin-playwright": "^2.2.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^61.0.2",
    "globals": "^16.4.0",
    "jasmine-core": "~5.10.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "8.40.0"
  }
}
